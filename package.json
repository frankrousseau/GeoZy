{
  "name": "geozy",
  "version": "0.2.0",
  "description": "simple tool for rendering GeoBookmarks files with OpenStreetMap",
  "main": "server.js",
  "bin": {
    "my-places": "bin/my-places"
  },
  "scripts": {
    "build": "coffee -cb -o build server* && cd client && npm run-script build && cd .. && mkdir -p build/client && cp -r client/public build/client",
    "test": "mocha test",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ChironGizmo/GeoZy.git"
  },
  "keywords": [
    "GeoBookmarks",
    "self-hosting",
    "single-page",
    "OSM",
    "react",
    "npm"
  ],
  "author": "Chiron Gizmo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChironGizmo/GeoZy/issues"
  },
  "homepage": "https://github.com/ChironGizmo/GeoZy#readme",
  "dependencies": {
    "americano": "^0.4.1",
    "async": "^0.9.0",
    "cozy-db-pouchdb": "0.0.13",
    "path-extra": "^1.0.2",
    "pouchdb": "^3.5.0"
  }
}
